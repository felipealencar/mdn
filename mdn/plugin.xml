<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="mdn"
            class="mdn.MdnPackage"/>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="%productName"
            application="mdn.diagram.MdnApplication">
         <property
               name="appName"
               value="%productName">
         </property>
         <property
               name="aboutImage"
               value="eclipse_lg.png">
         </property>
         <property
               name="aboutText"
               value="%productBlurb">
         </property>
         <property
               name="windowImages"
               value="eclipse16.png,eclipse32.png,eclipse48.png">
         </property>
         <property
               name="startupForegroundColor"
               value="9c9696">
         </property>
         <property
               name="startupMessageRect"
               value="7,265,320,20">
         </property>
         <property
               name="startupProgressRect"
               value="2,290,448,10">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>
   <extension
         id="mdneditor"
         point="org.eclipse.core.runtime.products">
      <product
            application="mdn.diagram.MdnApplication"
            name="mdn">
         <property
               name="appName"
               value="mdn">
         </property>
      </product>
   </extension>
   <extension
         id="mdn1"
         point="org.eclipse.core.runtime.products">
      <product
            application="mdn.diagram.MdnApplication"
            name="mdn">
         <property
               name="appName"
               value="mdn">
         </property>
      </product>
   </extension>

</plugin>
